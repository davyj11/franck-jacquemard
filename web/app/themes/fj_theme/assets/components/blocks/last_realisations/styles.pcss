
.last-realisations {

    .slider {
        .swiper-slide {
            @apply w-[232px] lg:w-[264px] !important;
        }
    }
    .w-btn {
        .control {
            @apply transition-all;
            @apply lg:hover:bg-primary lg:hover:text-primary-white;
            &.swiper-button-disabled {
                @apply pointer-events-none opacity-20;
            }
        }
    }
}
