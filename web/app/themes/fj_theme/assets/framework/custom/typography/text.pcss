@layer components {
    .p-s{
        @apply text-xs;
        @apply text-gray-01;
        @apply font-text;
        @apply font-normal;
        @apply font-text;
        @apply dark:text-white;
    }

    .p, p {
        @apply font-text;
        @apply text-base;
        @apply dark:text-white;
    }


    .p-md {
        @apply font-text;
        @apply text-md;
        @apply dark:text-white;
    }

    .p-lg {
        @apply font-text;
        @apply text-lg;
        @apply dark:text-white;
    }

    .link {
        @apply p dark:text-primary-white;
    }
}
